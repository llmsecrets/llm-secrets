{
  "name": "llm-secrets",
  "productName": "LLM Secrets",
  "version": "3.1.0",
  "versionNote": "Added scrt tx CLI for Ethereum transactions",
  "description": "Secure secrets management for LLM agents with Windows Hello / Touch ID protection",
  "main": ".webpack/main",
  "private": true,
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:mac": "electron-forge make --platform darwin",
    "make:win": "electron-forge make --platform win32",
    "build:touchid": "cd macos-helper && bash build.sh",
    "build:cli": "esbuild src/cli/tx.ts --bundle --platform=node --outfile=dist/cli/tx.js",
    "cli": "node dist/cli/tx.js",
    "cli:tx": "node dist/cli/tx.js tx",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "jest",
    "build": "electron-builder"
  },
  "keywords": ["encryption", "secrets", "env", "security", "windows-hello"],
  "author": {
    "name": "VestedJosh",
    "email": "josh@lendvest.io"
  },
  "license": "GPL-3.0",
  "build": {
    "appId": "com.vestedjosh.llm-secrets",
    "productName": "LLM Secrets",
    "win": {
      "target": ["nsis", "msi"],
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": ["dmg", "zip"],
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": ["AppImage", "deb"],
      "icon": "assets/icon.png"
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-webpack": "^7.11.1",
    "@electron/fuses": "^1.8.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "css-loader": "^6.11.0",
    "electron": "39.2.7",
    "electron-builder": "^26.4.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "jest": "^30.2.0",
    "node-loader": "^2.1.0",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "electron-squirrel-startup": "^1.0.1",
    "ethers": "^6.16.0",
    "keytar": "^7.9.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
